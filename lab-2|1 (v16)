#include <stdio.h>
#include <math.h>

int main() {
    double x, y, z, a, b, c;
    scanf("%lf", &x);
    scanf("%lf", &y);
    scanf("%lf", &z);
    if (x < 0){
        a = pow(x, 3);
    }
    if (x > 0){
        a = pow(x, 2);
    }
    else if (x == 0){
        a = (x + 1);
    }
    if (y < 0){
        b = pow(y, 3);
    }
    if (y > 0){
        b = pow(y, 2);
    }
    else if (y == 0){
        b = (y + 1);
    }
    if (z < 0){
        c = pow(z, 3);
    }
    if (z > 0){
        c = pow(z, 2);
    }
    else if (z == 0){
        c = (z + 1);
    }
    printf("%lg\n", a);
    printf("%lg\n", b);
    printf("%lg\n", c);
    
    return 0;
}
